{
  "name": "pokemon-save-web",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "pokemon-save-parser": "bin/pokemon-save-parser"
  },
  "files": [
    "bin/",
    "src/lib/parser/",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:website": "vitest run src/__tests__/health.test.ts src/__tests__/core-functionality.test.ts src/__tests__/integration-workflow.test.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:website && npm run build",
    "parse": "tsx src/lib/parser/cli.ts",
    "generate-mappings": "node scripts/generate-vanilla-mappings.js",
    "generate-icons": "tsx scripts/generate-icons.ts && tsx scripts/generate-og-image.ts",
    "mgba:start": "docker compose -f docker/docker-compose.yml up -d",
    "mgba:start:build": "docker compose -f docker/docker-compose.yml up -d --build",
    "mgba:run": "docker compose -f docker/docker-compose.yml up",
    "mgba:run:build": "docker compose -f docker/docker-compose.yml up --build",
    "mgba:stop": "docker compose -f docker/docker-compose.yml down",
    "mgba:build": "docker build -f docker/Dockerfile -t mgba-test-env:latest .",
    "mgba:pull": "docker pull ghcr.io/johndeved/pokemon-save-web/mgba-test-env:latest"
  },
  "dependencies": {
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-three/fiber": "^9.1.4",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.81.5",
    "@types/react-query": "^1.2.8",
    "@undefined/lint": "github:JohnDeved/undefined-lint",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.0",
    "lucide-react": "^0.525.0",
    "next-themes": "^0.4.6",
    "react-dropzone": "^14.3.8",
    "react-icons": "^5.5.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "three": "^0.178.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/three": "^0.177.0",
    "@types/wicg-file-system-access": "^2023.10.6",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/ui": "^3.2.4",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "cheerio": "^1.1.2",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sharp": "^0.34.3",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.5",
    "typescript": "~5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-pwa": "^1.0.2",
    "vitest": "^3.2.4",
    "workbox-window": "^7.3.0",
    "ws": "^8.18.3"
  }
}
