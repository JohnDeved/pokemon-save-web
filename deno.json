{
  "lock": false,
  "nodeModulesDir": "none",
  "unstable": ["webgpu"],
  "tasks": {
    "dev": "deno run --allow-net --allow-read --allow-write --allow-env ./scripts/dev.ts",
    "build": "deno run --allow-net --allow-read --allow-write --allow-env ./scripts/build.ts",
    "preview": "deno run --allow-net --allow-read --allow-write --allow-env ./scripts/preview.ts",
    "test": "deno test --allow-net --allow-read --allow-write --allow-env",
    "test:watch": "deno test --allow-net --allow-read --allow-write --allow-env --watch",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "parse": "deno run --allow-net --allow-read --allow-write --allow-env ./src/lib/parser/cli.ts",
    "generate-mappings": "deno run --allow-net --allow-read --allow-write --allow-env ./scripts/generate-vanilla-mappings.ts",
    "generate-icons": "deno run --allow-net --allow-read --allow-write --allow-env ./scripts/generate-icons.ts && deno run --allow-net --allow-read --allow-write --allow-env ./scripts/generate-og-image.ts",
    "mgba": "deno run --allow-net --allow-read --allow-write --allow-env ./docker/mgba-docker.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.0",
    "@std/testing": "jsr:@std/testing@^1.0.0",
    "@std/path": "jsr:@std/path@^1.0.0",
    "@std/fs": "jsr:@std/fs@^1.0.0",
    "@std/http": "jsr:@std/http@^1.0.0",
    "@std/encoding": "jsr:@std/encoding@^1.0.0",
    "react": "https://esm.sh/react@19.1.0",
    "react-dom": "https://esm.sh/react-dom@19.1.0",
    "react-dom/client": "https://esm.sh/react-dom@19.1.0/client",
    "@tanstack/react-query": "https://esm.sh/@tanstack/react-query@5.81.5",
    "@radix-ui/react-menubar": "https://esm.sh/@radix-ui/react-menubar@1.1.15",
    "@radix-ui/react-select": "https://esm.sh/@radix-ui/react-select@2.2.5",
    "@radix-ui/react-slider": "https://esm.sh/@radix-ui/react-slider@1.3.5",
    "@radix-ui/react-slot": "https://esm.sh/@radix-ui/react-slot@1.2.3",
    "@radix-ui/react-tooltip": "https://esm.sh/@radix-ui/react-tooltip@1.2.7",
    "@react-three/fiber": "https://esm.sh/@react-three/fiber@9.1.4",
    "three": "https://esm.sh/three@0.178.0",
    "framer-motion": "https://esm.sh/framer-motion@12.23.0",
    "lucide-react": "https://esm.sh/lucide-react@0.525.0",
    "react-dropzone": "https://esm.sh/react-dropzone@14.3.8",
    "react-icons": "https://esm.sh/react-icons@5.5.0",
    "file-saver": "https://esm.sh/file-saver@2.0.5",
    "sonner": "https://esm.sh/sonner@2.0.6",
    "next-themes": "https://esm.sh/next-themes@0.4.6",
    "clsx": "https://esm.sh/clsx@2.1.1",
    "class-variance-authority": "https://esm.sh/class-variance-authority@0.7.1",
    "tailwind-merge": "https://esm.sh/tailwind-merge@3.3.1",
    "isomorphic-ws": "https://esm.sh/isomorphic-ws@5.0.0",
    "zod": "https://esm.sh/zod@3.25.67"
  },
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "lib": ["deno.window", "dom", "dom.iterable", "dom.asynciterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 120,
    "indentWidth": 2,
    "semiColons": false,
    "singleQuote": true,
    "proseWrap": "preserve",
    "include": ["src/", "scripts/", "docker/"],
    "exclude": ["node_modules/", "dist/", "public/"]
  },
  "lint": {
    "include": ["src/", "scripts/", "docker/"],
    "exclude": ["node_modules/", "dist/", "public/"]
  }
}