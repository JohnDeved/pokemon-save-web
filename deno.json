{
  "lock": false,
  "nodeModulesDir": "auto",
  "unstable": ["webgpu"],
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read jsr:@std/http@1/file-server dist/",
    "test": "deno test --allow-net --allow-read --allow-write --allow-env",
    "test:watch": "deno test --allow-net --allow-read --allow-write --allow-env --watch",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "parse": "deno run --unstable-sloppy-imports --allow-net --allow-read --allow-write --allow-env ./src/lib/parser/cli.ts",
    "generate-mappings": "deno run --allow-net --allow-read --allow-write --allow-env ./scripts/generate-vanilla-mappings.ts",
    "generate-icons": "deno run --allow-net --allow-read --allow-write --allow-env ./scripts/generate-icons.ts && deno run --allow-net --allow-read --allow-write --allow-env ./scripts/generate-og-image.ts",
    "mgba": "deno run --allow-net --allow-read --allow-write --allow-env ./docker/mgba-docker.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.0",
    "@std/testing": "jsr:@std/testing@^1.0.0",
    "@std/path": "jsr:@std/path@^1.0.0",
    "@std/fs": "jsr:@std/fs@^1.0.0",
    "@std/http": "jsr:@std/http@^1.0.0",
    "@std/encoding": "jsr:@std/encoding@^1.0.0",
    "clsx": "https://esm.sh/clsx@2.1.1",
    "tailwind-merge": "https://esm.sh/tailwind-merge@3.3.1",
    "class-variance-authority": "https://esm.sh/class-variance-authority@0.7.1",
    "zod": "https://esm.sh/zod@3.25.67",
    "zustand": "https://esm.sh/zustand@5.0.2",
    "sonner": "https://esm.sh/sonner@2.0.6",
    "framer-motion": "https://esm.sh/framer-motion@12.23.0",
    "lucide-react": "https://esm.sh/lucide-react@0.525.0",
    "file-saver": "https://esm.sh/file-saver@2.0.5",
    "file-selector": "https://esm.sh/file-selector@0.6.0",
    "react-dropzone": "https://esm.sh/react-dropzone@14.3.8",
    "react-icons/fa6": "https://esm.sh/react-icons@5.5.0/fa6",
    "react-icons/hi": "https://esm.sh/react-icons@5.5.0/hi", 
    "react-icons/io5": "https://esm.sh/react-icons@5.5.0/io5",
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.5",
    "@types/react": "npm:@types/react@^19.1.9",
    "@types/react-dom": "npm:@types/react-dom@^19.1.7",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.7.0",
    "react": "npm:react@^19.1.1",
    "react-dom": "npm:react-dom@^19.1.1",
    "vite": "npm:vite@^7.0.6",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.11",
    "tailwindcss": "npm:tailwindcss@^4.1.11",
    "vite-plugin-pwa": "npm:vite-plugin-pwa@^1.0.2",
    "babel-plugin-react-compiler": "npm:babel-plugin-react-compiler@^19.1.0-rc.2",
    "@types/file-saver": "npm:@types/file-saver@^2.0.7",
    "@types/node": "npm:@types/node@^24.1.0",
    "@types/three": "npm:@types/three@^0.177.0",
    "three": "npm:three@^0.178.0",
    "@react-three/fiber": "npm:@react-three/fiber@^9.1.4",
    "@radix-ui/react-menubar": "npm:@radix-ui/react-menubar@^1.1.15",
    "@radix-ui/react-select": "npm:@radix-ui/react-select@^2.2.5",
    "@radix-ui/react-slider": "npm:@radix-ui/react-slider@^1.3.5",
    "@radix-ui/react-slot": "npm:@radix-ui/react-slot@^1.2.3",
    "@radix-ui/react-tooltip": "npm:@radix-ui/react-tooltip@^1.2.7",
    "@tanstack/react-query": "npm:@tanstack/react-query@^5.81.5",
    "isomorphic-ws": "npm:isomorphic-ws@^5.0.0",
    "next-themes": "npm:next-themes@^0.4.6"
  },
  "compilerOptions": {
    "strict": true,
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react"
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 120,
    "indentWidth": 2,
    "semiColons": false,
    "singleQuote": true,
    "proseWrap": "preserve",
    "include": ["src/", "scripts/", "docker/"],
    "exclude": ["node_modules/", "dist/", "public/"]
  },
  "lint": {
    "include": ["src/", "scripts/", "docker/"],
    "exclude": ["node_modules/", "dist/", "public/"]
  }
}